<div class="modal" id="employmentModal">
    <div class="modal-dialog   indprofModal">
        <div class="modal-content">
            <div class="modal-header">
                <h6 class="modal-title">Add Employment History </h6>
            </div>
            <form id="empForm">
                <div class="modal-body">
                    <div class="form-group row mb-md-2 ">
                        <label class="control-label col-sm-4"> Employment Category</label>
                        <select class="form-control  form-input col-sm-8" id="empCategory" required asp-items=ViewBag.EmploymentCategories>
                            <option value="" selected disabled>Select Employment Category</option>
                        </select>
                        <span class="error empCategoryerror col-sm-12 text-right  pr-0 mr-0">Employment Category is Required</span>
                    </div>

                    <div class="form-group row mb-md-2 required">
                        <label class="control-label col-sm-4">Company Name</label>
                        <input type="text" class="form-control form-input col-sm-8" id="employer" required />
                        <span class="error employererror col-sm-12 text-right  pr-0 mr-0">Company Name is Required</span>
                    </div>

                    <div class="form-group row mb-md-2 required">
                        <label class="control-label col-sm-4">Job Title</label>
                        <input type="text" class="form-control form-input col-sm-8" id="empDesignation" required />
                        <span class="error empDesignationerror col-sm-12 text-right  pr-0 mr-0">Designation is Required</span>
                    </div>

                    <div class="form-group row mb-md-2">
                        <label class="control-label col-sm-4">Start Date</label>
                        <input type="date" class="form-control form-input col-sm-8" id="empStartDate" />

                    </div>

                    <div class="form-group row mb-md-2">
                        <label class="control-label col-sm-4">End Date</label>
                        <input type="date" class="form-control form-input col-sm-8" id="empEndDate" />
                    </div>

                    <div class="form-group row mb-md-2">
                        <label class="control-label col-sm-4">Company Phone Number</label>
                        <input type="tel" class="form-control form-input col-sm-8" id="empTelNumber" />
                    </div>

                    <div class="form-group row mb-md-2">
                        <label class="control-label col-sm-4">Email</label>
                        <input type="email" class="form-control form-input col-sm-8" id="empCompEmail" />
                    </div>

                    <div class="form-group row">
                        <label class="control-label col-sm-4">Job Title</label>
                        <input type="email" class="form-control form-input col-sm-8" id="empDuties" />
                    </div>

                </div>
                <!-- Modal footer -->
                <div class="modal-footer  border-top-0 mr-4">
                    <button type="button"
                            class="btn btn-primary"
                            id="addEmpHistory">
                        Add
                    </button>
                    <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
