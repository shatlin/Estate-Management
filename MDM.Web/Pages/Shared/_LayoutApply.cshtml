    <style>
        .table > thead > tr > th,
        .table > tbody > tr > th,
        .table > tfoot > tr > th,
        .table > thead > tr > td,
        .table > tbody > tr > td,
        .table > tfoot > tr > td {
            padding: 3px 3px;
        }

        .content {
            padding-left: 50px;
            padding-right: 50px;
            padding-top: 20px;
            padding-bottom: 50px;
        }

        .nav-tabs-navigation .nav > .nav-item > .nav-link {
            padding-bottom: 10px;
        }

        body {
            font-size: 12px;
        }

        .card label {
            font-size: 0.96em;
        }

        h6 {
            text-transform: none;
        }

        .form-control {
            font-size: 12px;
        }
    </style>
