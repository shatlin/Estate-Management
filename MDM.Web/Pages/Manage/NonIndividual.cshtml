<form id="NonMemberForm" method="post" name="NonMemberForm" enctype="multipart/form-data">
    <input type="hidden" class="form-control" asp-for=Organization.OrgMemberCode />
    <input type="hidden" class="form-control" asp-for=Organization.Id />
    <div class="row regMemberForm" id="regMemberForm">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header text-center">
                    <h5 class="card-title">
                        Manage Non-Individual - @Model.Organization.Name
                    </h5>

                    <div class="row">
                        <div class="col-3 font-weight-bolder text-left text-primary">Membeship Number: @Model.Organization.OrgMemberCode</div>

                        <div class="col-3 font-weight-bolder text-center  text-primary">
                            
                            @if (Model.PrimaryContactId != null)
                            {
                            <a class="control-label" href="/Manage/Member?Id=@Model.PrimaryContactId">Primary Contact: @Model.PrimaryContactName</a>
                            }
                        </div>

                        <div class="col-3 font-weight-bolder text-center  text-primary">
                           
                            @if (Model.ContactPersonId != null)
                            {
                            <a class="control-label" href="/Manage/Member?Id=@Model.ContactPersonId"> Contact Person: @Model.ContactPersonName</a>
                            }
                        </div>

                        @if (@Model.Organization.AccountBalance == null)
                        {
                            <div class="col-3 font-weight-bolder text-right  text-primary"> Account Balance: 0.00</div>
                        }
                        else
                        {
                            <div class="col-3 font-weight-bolder text-right  text-primary"> Account Balance: @Model.Organization.AccountBalance</div>
                        }
                    </div>
                    <br />


                </div>







