@model MM.Pages.Manage.IndividualModel

@{

    ViewData["Title"] = "Manage Individual";
    ViewData["Page"] = "Manage Member";

    bool isAuthorizedForDelete = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowSetupDelete).Result.Succeeded;
    bool isAuthorizedForUpdate = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowSetupUpdate).Result.Succeeded;
    string unspecifiedFile = "unspecified";
    string active = "";
    string user = @UserManager.FindByNameAsync(UserManager.GetUserName(User)).Result.FullName;
    var isSuperUser = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowSuperUser).Result.Succeeded;
    var isAdminUser = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowAdminUser).Result.Succeeded;
  
}

<style>
    body {
        background-color: #f0f5f5;
    }

    .main-panel > .content {
        margin-top: 75px;
    }

    .table > thead > tr > th,
    .table > tbody > tr > th,
    .table > tfoot > tr > th,
    .table > thead > tr > td,
    .table > tbody > tr > td,
    .table > tfoot > tr > td {
        padding: 3px 3px;
    }

    .ajax-loader {
        visibility: hidden;
        background-color: rgba(255,255,255,0.7);
        position: absolute;
        z-index: +100 !important;
        width: 100%;
        height: 100%;
    }

        .ajax-loader img {
            position: relative;
            top: 50%;
            left: 50%;
        }

    .table > tbody > tr > td,
    .table > tbody > tr > th {
        border-top: none;
    }

    .table {
        border-top: none;
    }

    .main-panel {
        padding-top: 15px;
    }

    .error {
        color: #ef8157 !important;
        display: none;
        font-size: 12px;
    }

    .regMemberForm .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.5);
    }

    .regMemberForm .disability-align {
        margin-bottom: 20px;
    }

    .regMemberForm .banking-icon {
        font-size: 25px;
        margin-left: 5px;
        color: #22b9ff;
    }

    .regMemberForm #otherCountry {
        visibility: hidden;
    }

    /*    .regMemberForm #addEmployment {
        display: none;
    }

    .regMemberForm #addEmploymentbutton {
        display: none;
    }*/

    .regMemberForm .delbtn {
        cursor: pointer;
    }

    input.input-validation-error {
        background-color: #FFC0A4;
        border: 1px solid #ef8157;
    }

    select.input-validation-error {
        background-color: #FFC0A4;
        border: 1px solid #ef8157;
    }

    .text-danger {
        font-size: 12px;
    }

    .required ::after {
        content: " *" !important;
        color: red !important;
    }

    .input-group, .form-group {
        margin-bottom: 2px;
    }

        .form-group .form-control, .input-group .form-control {
            padding: 5px 5px 5px 5px;
            font-size: 12px;
        }

    .regMemberForm .custom-file-label {
        overflow: hidden;
        white-space: nowrap;
    }

    .nav-top-style {
        padding-top: 0px !important;
    }

    .indprofModal {
        max-width: 600px;
    }

    .card label {
        margin-top: 5px;
    }

    input::-webkit-input-placeholder {
        font-size: 10px;
        line-height: 3;
    }

    ::-moz-placeholder {
        font-size: 11px;
        line-height: 3;
    }

    :-ms-input-placeholder {
        font-size: 11px;
        line-height: 3;
    }
</style>

