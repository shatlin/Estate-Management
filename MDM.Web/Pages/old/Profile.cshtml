<style>
    body {
        background-color: #f0f5f5;
    }

    .main-panel > .content {
        margin-top: 75px;
    }

    .table > thead > tr > th,
    .table > tbody > tr > th,
    .table > tfoot > tr > th,
    .table > thead > tr > td,
    .table > tbody > tr > td,
    .table > tfoot > tr > td {
        padding: 3px 3px;
    }

    .main-panel {
        padding-top: 15px;
    }

    .error {
        color: red;
        display: none;
    }

    .ind-member .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.5);
    }

    .membership-card {
        background: #22b9ff;
    }

    .membership-select {
        width: 50%;
        margin: 10px 0 10px 25%;
        font-size: 20px;
        height: auto !important;
    }

    .disability-align {
        margin-bottom: 20px;
    }

    .ind-member .hr-line {
        border: none;
        border-bottom: 1px solid #22b9ff;
    }

    .ind-member .vl {
        border-left: 1px solid #22b9ff;
    }

    .ind-member .vl-addinfo {
        border-left: 1px solid #22b9ff;
        height: 370px;
    }

    .ind-member .agree-checkbox {
        zoom: 1.3;
    }

    .ind-member .banking-icon {
        font-size: 25px;
        margin-left: 5px;
        color: #22b9ff;
    }

    .ind-member .register-btn {
        width: 15%;
    }

    .ind-member .addDoc-modal {
        margin-top: 10%;
    }

    .ind-member #otherCountry {
        display: none;
    }

    .ind-member #addEmployment {
        display: none;
    }

    .ind-member #selectFile {
        display: none;
    }

    .ind-member #addSelFileBtn {
        display: none;
    }

    .ind-member .delbtn {
        cursor: pointer;
    }

    .ind-member .table-scroll {
        height: 185px !important;
        overflow: auto;
    }

    .ind-member .form-group .required .control-label:after {
        content: " *" !important;
        color: red !important;
    }

    .input-group, .form-group {
        margin-bottom: 2px;
    }

        .form-group .form-control, .input-group .form-control {
            padding: 5px 5px 5px 5px;
        }

    .ind-member .custom-file-label {
        overflow: hidden;
        white-space: nowrap;
    }

    .card label{
        margin-top: 7px;
    }

    .nav-top-style {
        padding-top: 0px !important;
    }

    input::-webkit-input-placeholder {
        font-size: 10px;
        line-height: 3;
    }

    ::-moz-placeholder {
        font-size: 11px;
        line-height: 3;
    }

    :-ms-input-placeholder {
        font-size: 11px;
        line-height: 3;
    }

    .dot-ul {
        list-style: none;
    }

    .dot-ul li::before {
        content: "\2022";
        color:aqua;
        font-weight: bold;
        display: inline-block;
        width: 1em;
        margin-left: -1em;
    }

</style>

