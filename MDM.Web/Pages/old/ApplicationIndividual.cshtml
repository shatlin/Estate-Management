        /****** Show Select file input ************/
        function showSelectDiv(selectedOption) {
            if (selectedOption.value !== "") {
                document.getElementById("selectFile").style.display = "block";
            }
        }

        /******* Display name of file *******/
        $(".custom-file-input").on("change", function () {
            var files = [];
            for (var i = 0; i < $(this)[0].files.length; i++) {
                files.push($(this)[0].files[i].name);
            }
            $(this)
                .siblings(".custom-file-label")
                .addClass("selected")
                .html(files.join(', '));
        });

        /******** Upload Button ********/
        function uploadFile() {
            document.getElementById("addSelFileBtn").style.display = "none";

            var selectedList = document.getElementById("selFileType");
            var fileType = selectedList.options[selectedList.selectedIndex].value;

            var fileName = document.getElementById("customFile").files[0].name;

            var tableRef = document.getElementById("documentsTable");

            // Insert a row in the table at the last row
            var newRow = tableRef.insertRow();

            // Insert a cell in the row at index 0
            var newFileType = newRow.insertCell(0);
            var newFileName = newRow.insertCell(-1);
            var deleteBtn = newRow.insertCell(2);

            // Append a text node to the cell
            var newText1 = document.createTextNode(fileType);
            var newText2 = document.createTextNode(fileName);
            var button = document.createElement("i");

            //set the style
            button.className = "fa fa-trash delbtn";

            // add button's "onclick" event.
            button.setAttribute("onclick", "removeRow(this)");

            newFileType.appendChild(newText1);
            newFileName.appendChild(newText2);
            deleteBtn.appendChild(button);

            //Reset the 2 fields
            $("#selFileType option").prop("selected", function () {
                return this.defaultSelected;
            });
            $(".custom-file-input").on("change", function () {
                var fileName = "";
                $(this)
                    .siblings(".custom-file-label")
                    .addClass("selected")
                    .html(fileName);
            });
            //document.getElementById('customFile').value = '';
            //$("#customFile").val("");
        }

