@page
@model WISA.Pages.Member.ApplicationIndividualModel
@{

    Layout = null;
    ViewData["Title"] = "Individual Application";
    bool isAuthorizedForDelete = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowSetupDelete).Result.Succeeded;
    bool isAuthorizedForUpdate = AuthorizationService.AuthorizeAsync(User, MMPolicies.AllowSetupUpdate).Result.Succeeded;

}

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css" />
<link rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.0.0/animate.min.css" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css" />
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" />
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css" />
<link href='https://fonts.googleapis.com/css?family=Montserrat' rel='stylesheet'>
<script src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
<script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/js/all.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
<script src="~/js/bootstrap-notify.js"></script>

<!-- StyleSheet -->
<style>
    body {
        background-color: #f0f5f5;
    }

    .error {
        color: red;
        display: none;
    }

    .ind-member .card {
        box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.5);
    }

    .membership-card {
        background: #22b9ff;
    }

    .membership-select {
        width: 50%;
        margin: 10px 0 10px 25%;
        font-size: 20px;
        height: auto !important;
    }

    .disability-align {
        margin-bottom: 20px;
    }

    .ind-member .hr-line {
        border: none;
        border-bottom: 1px solid #22b9ff;
    }

    .ind-member .vl {
        border-left: 1px solid #22b9ff;
    }

    .ind-member .vl-addinfo {
        border-left: 1px solid #22b9ff;
        height: 370px;
    }

    .ind-member .agree-checkbox {
        zoom: 1.3;
    }

    .ind-member .banking-icon {
        font-size: 25px;
        margin-left: 5px;
        color: #22b9ff;
    }

    .ind-member .register-btn {
        width: 15%;
    }

    .ind-member .addDoc-modal {
        margin-top: 10%;
    }

    .ind-member #otherCountry {
        display: none;
    }

    .ind-member #addEmployment {
        display: none;
    }

    .ind-member #selectFile {
        display: none;
    }

    .ind-member #addSelFileBtn {
        display: none;
    }

    .ind-member .delbtn {
        cursor: pointer;
    }

    .ind-member .table-scroll {
        height: 185px !important;
        overflow: auto;
    }

    .ind-member .form-group.required .control-label:after {
        content: " *";
        color: red;
    }

    .ind-member .custom-file-label {
        overflow: hidden;
        white-space: nowrap;
    }
</style>

<div class="container-fluid mt-4 ind-member">
    <h5 class="display-4 text-center">Individual Membership</h5>
    <form method="post" enctype="multipart/form-data">
        <!-- First Row -->
        <div class="row">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <!-- First Col -->
            <div class="col-6">
                <!-- Profile info -->
                <div class="card mb-4">
                    <div class="card-header">
                        <h6><b>Profile info</b></h6>
                    </div>

                    <div class="card-body" style="margin-bottom: 115px;">
                        <!-- Title/Initials -->
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.TitleId class="control-label"></label>

                                    <select class="form-control form-input" asp-for=ApplicationIndividualVM.TitleId asp-items=ViewBag.Titles>
                                        <option value="" selected disabled>Select title</option>

                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.TitleId" class="text-danger"></span>
                                </div>
                            </div>


                            <div class="col-6">
                                <div class="form-group">
                                    <label asp-for=ApplicationIndividualVM.Initials class="control-label"></label>
                                    <input type="text" class="form-control  form-input" asp-for=ApplicationIndividualVM.Initials />
                                    <span asp-validation-for="ApplicationIndividualVM.Initials" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <!-- FirstName -->
                        <div class="form-group required">
                            <label asp-for=ApplicationIndividualVM.FirstName class="control-label"></label>
                            <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.FirstName />
                            <span asp-validation-for="ApplicationIndividualVM.FirstName" class="text-danger"></span>
                        </div>

                        <!-- LastName -->
                        <div class="form-group required">
                            <label asp-for=ApplicationIndividualVM.LastName class="control-label"></label>
                            <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.LastName />
                            <span asp-validation-for="ApplicationIndividualVM.LastName" class="text-danger"></span>
                        </div>

                        <!-- Gender/BirthDate -->
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.GenderId class="control-label"></label>
                                    <select class="form-control form-input" asp-for=ApplicationIndividualVM.GenderId asp-items=ViewBag.Genders>
                                        <option value="" selected disabled>Select Gender</option>
                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.GenderId" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.BirthDay class="control-label"></label>
                                    <input type="date" class="form-control form-input" asp-for=ApplicationIndividualVM.BirthDay />
                                    <span asp-validation-for="ApplicationIndividualVM.BirthDay" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <!-- Highest Qualification -->
                        <div class="form-group">
                            <label asp-for=ApplicationIndividualVM.HighestQualificationId class="control-label"></label>
                            <select class="form-control form-input" asp-for=ApplicationIndividualVM.HighestQualificationId asp-items=ViewBag.Qualifications>
                                <option value="" selected disabled>Select Qualification</option>
                            </select>
                            <span asp-validation-for="ApplicationIndividualVM.HighestQualificationId" class="text-danger"></span>
                        </div>

                        <!-- Occupation -->
                        <div class="form-group required">
                            <label asp-for=ApplicationIndividualVM.OccupationId class="control-label"></label>
                            <select class="form-control form-input" asp-for=ApplicationIndividualVM.OccupationId asp-items=ViewBag.Occupations>
                                <option value="" selected disabled>Select Occupation</option>
                            </select>
                            <span asp-validation-for="ApplicationIndividualVM.OccupationId" class="text-danger"></span>
                        </div>

                        <!-- CompanyName -->
                        <div class="form-group required">
                            <label asp-for=ApplicationIndividualVM.CompanyName class="control-label"></label>
                            <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.CompanyName />
                            <span asp-validation-for="ApplicationIndividualVM.CompanyName" class="text-danger"></span>
                        </div>

                        <!-- JobTitle -->
                        <div class="form-group">
                            <label asp-for=ApplicationIndividualVM.JobTitle class="control-label"></label>
                            <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.JobTitle />
                            <span asp-validation-for="ApplicationIndividualVM.JobTitle" class="text-danger"></span>
                        </div>

                        <!-- HomeLang/Ethnicity -->
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.HomeLanguageId class="control-label"></label>
                                    <select class="form-control form-input" asp-for=ApplicationIndividualVM.HomeLanguageId asp-items=ViewBag.HomeLanguages>
                                        <option value="" selected disabled>Select Language</option>
                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.HomeLanguageId" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.EthnicityId class="control-label"></label>
                                    <select class="form-control form-input" asp-for=ApplicationIndividualVM.EthnicityId asp-items=ViewBag.Ethnicities>
                                        <option value="" selected disabled>Select Ethnicity</option>
                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.EthnicityId" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <!-- Id/Nationality -->
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.IDNumber class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.IDNumber />
                                    <span asp-validation-for="ApplicationIndividualVM.IDNumber" class="text-danger"></span>
                                </div>
                            </div>

                            <div class="col-6">
                                <div class="form-group required">
                                    <label asp-for=ApplicationIndividualVM.NationalityId class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=ApplicationIndividualVM.NationalityId asp-items=ViewBag.Nationalities
                                            onchange="showSelNationality(this)">
                                        <option value="" selected disabled>Select Nationality</option>
                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.NationalityId" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <!-- Other Country -->
                        <div class="row">
                            <div class="col-6"></div>
                            <div class="col-6">
                                <div class="form-group required" id="otherCountry">
                                    <label asp-for=ApplicationIndividualVM.CountryId class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=ApplicationIndividualVM.CountryId asp-items=ViewBag.Countries>
                                        <option value="" selected disabled>Select Country</option>
                                    </select>
                                    <span asp-validation-for="ApplicationIndividualVM.CountryId" class="text-danger"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- / Profile info -->
            </div>
            <!-- / First Col -->
            <!-- Second Col -->
            <div class="col-6">
                <!-- Contact info -->
                <div class="card mb-4">
                    <div class="card-header">
                        <h6><b>Contact info</b></h6>
                    </div>

                    <div class="card-body">
                        <div class="row">
                            <div class="col-4">
                                <div class="form-group">
                                    <label asp-for=ApplicationIndividualVM.HomePhoneNumber class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.HomePhoneNumber />
                                </div>
                            </div>

                            <div class="col-4">
                                <div class="form-group">
                                    <label asp-for=ApplicationIndividualVM.BusinessPhoneNumber class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.BusinessPhoneNumber />
                                </div>
                            </div>

                            <div class="col-4 form-group required">
                                <label asp-for=ApplicationIndividualVM.MobilePhone class="control-label"></label>
                                <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.MobilePhone />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-6 form-group required">
                                <label asp-for=ApplicationIndividualVM.Email class="control-label"></label>
                                <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.Email />
                            </div>

                            <div class="col-6 form-group">
                                <label asp-for=ApplicationIndividualVM.SecondaryEmail class="control-label"></label>
                                <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.SecondaryEmail />
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-6">
                                <div class="form-group">
                                    <label asp-for=ApplicationIndividualVM.FAXNumber class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.FAXNumber />
                                </div>
                            </div>

                            <div class="col-6">
                                <div class="form-group">
                                    <label asp-for=ApplicationIndividualVM.FaxToEmail class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=ApplicationIndividualVM.FaxToEmail />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- / Contact info -->
                <!-- Physical/Postal row -->
                <div class="row">
                    <!-- Physical Address -->
                    <div class="col-6">
                        <div class="card mb-4">
                            <div class="card-header">
                                <h6><b>Physical Address</b></h6>
                            </div>

                            <div class="card-body">
                                <div class="form-group">
                                    <label asp-for=physicaladress.AddressLine1 class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=physicaladress.AddressLine1 />
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.AddressLine2 class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=physicaladress.AddressLine2 />
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.Suburb class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=physicaladress.Suburb />
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.City class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=physicaladress.City />
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.Province class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=physicaladress.Province asp-items=ViewBag.Provinces>
                                        <option value="" selected disabled>Select Province</option>

                                    </select>
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.CountryId class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=physicaladress.CountryId asp-items=ViewBag.Countries>
                                        <option value="" selected disabled>Select Country</option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label asp-for=physicaladress.PostalCode class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=physicaladress.PostalCode />
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- / Physical Address -->
                    <!-- Postal Address -->
                    <div class="col-6">
                        <div class="card mb-4">
                            <div class="card-header">
                                <h6><b>Postal Address</b></h6>
                            </div>

                            <div class="card-body">
                                <div class="form-group">
                                    <label asp-for=postaladress.AddressLine1 class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=postaladress.AddressLine1 />
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.AddressLine2 class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=postaladress.AddressLine2 />
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.Suburb class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=postaladress.Suburb />
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.City class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=postaladress.City />
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.Province class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=postaladress.Province asp-items=ViewBag.Provinces>
                                        <option value="" selected disabled>Select Province</option>

                                    </select>
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.CountryId class="control-label"></label>
                                    <select class="form-control  form-input" asp-for=postaladress.CountryId asp-items=ViewBag.Countries>
                                        <option value="" selected disabled>Select Country</option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label asp-for=postaladress.PostalCode class="control-label"></label>
                                    <input type="text" class="form-control form-input" asp-for=postaladress.PostalCode />
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- / Postal Address -->
                </div>
                <!-- / Physical/Postal row -->
            </div>
            <!-- / Second Col -->
        </div>
        <!-- / First Row -->
        <!-- Second Row -->
        <div class="row">
            <!-- Qualifications -->
            <div class="col-6">
                <div class="card mb-4">
                    <div class="card-header">
                        <h6><b>Qualifications</b></h6>
                    </div>

                    <div class="card-body">
                        <button type="button"
                                class="btn btn-primary mb-2"
                                data-toggle="modal"
                                data-target="#qualificationModal">
                            Add Qualification
                        </button>

                        <table class="table table-sm" id="qualificationTable">
                            <thead>
                                <tr>
                                    <th>Institute</th>
                                    <th>Qualification Name</th>
                                    <th>Qualification Type</th>
                                    <th></th>
                                </tr>
                            </thead>

                            <tbody></tbody>
                        </table>
                    </div>
                </div>

                <!-- Qualification Modal -->
                <div class="modal" id="qualificationModal">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h4 class="modal-title">Add Qualification</h4>
                                <button type="button" class="close" data-dismiss="modal">
                                    &times;
                                </button>
                            </div>

                            <div class="modal-body">
                                <div class="form-group">
                                    <label class="control-label">Qualification Category</label>
                                    <select class="form-control  form-input" id="qualCategory" asp-items=ViewBag.QualificationCategories>
                                        <option value="" selected disabled>Select Qualification Category</option>
                                    </select>
                                </div>

                                <div class="form-group">
                                    <label class="control-label">Institue</label>
                                    <input type="text" class="form-control form-input" id="qualInstitute" />
                                </div>

                                <div class="form-group">
                                    <label class="control-label">Qualification</label>
                                    <input type="text" class="form-control form-input" id="qualName" />

                                </div>

                                <div class="form-group">
                                    <label class="control-label">Student Number</label>
                                    <input type="text" class="form-control form-input" id="qualStudNumber" />

                                </div>

                                <div class="form-group">
                                    <label class="control-label">Start Date</label>
                                    <input type="date" class="form-control form-input" id="qualStartDate" />

                                </div>

                                <div class="form-group">
                                    <label class="control-label">End Date</label>
                                    <input type="date" class="form-control form-input" id="qualEndDate" />

                                </div>

                                <div class="form-group">
                                    <label class="control-label">Qualification Type</label>
                                    <select class="form-control  form-input" id="qualType" asp-items=ViewBag.QualificationTypes>
                                        <option value="-1" selected disabled>Select Qualification Type</option>

                                    </select>
                                </div>

                                <div class="form-group">
                                    <label class="control-label">Enrolement Category</label>
                                    <select class="form-control  form-input" id="qualEnrolCategory" asp-items=ViewBag.EnrolmentCategories>
                                        <option value="" selected disabled>Select Enrolment Category</option>
                                    </select>
                                </div>
                            </div>

                            <!-- Modal footer -->
                            <div class="modal-footer">
                                <button type="button"
                                        class="btn btn-primary"
                                        data-dismiss="modal"
                                        id="addQualification">
                                    Add
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- / Qualification Modal-->
            </div>
            <!-- / Qualifications -->
            <!-- Employment -->
            <div class="col-6">
                <div class="card mb-4">
                    <div class="card-header">
                        <h6><b>Employment History</b></h6>
                    </div>

                    <div class="card-body" style="margin-bottom: 11px;">
                        <div class="form-group">
                            <label>Do you have any Employment History?</label>
                            <select class="form-control"
                                    id="selEmpHistory"
                                    onchange="showSelAddEmp(this)">
                                <option value="" selected disabled>Select</option>
                                <option value=false>Yes</option>
                                <option value=true>No</option>
                            </select>
                        </div>

                        <div id="addEmployment">
                            <button type="button"
                                    class="btn btn-primary mb-2"
                                    data-toggle="modal"
                                    data-target="#employmentModal">
                                Add Employment
                            </button>

                            <table class="table table-sm" id="employmentTable">
                                <thead>
                                    <tr>
                                        <th>Employer</th>
                                        <th>Designation</th>
                                        <th>Employment Category</th>
                                        <th></th>
                                    </tr>
                                </thead>

                                <tbody></tbody>
                            </table>
                        </div>
                    </div>

                    <!-- Employment Modal -->
                    <div class="modal" id="employmentModal">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title">Add Employment History</h4>
                                    <button type="button" class="close" data-dismiss="modal">
                                        &times;
                                    </button>
                                </div>

                                <div class="modal-body">
                                    <div class="form-group">
                                        <label class="control-label"> Employment Category</label>
                                        <select class="form-control  form-input" id="empCategory" asp-items=ViewBag.EmploymentCategories>
                                            <option value="" selected disabled>Select Employment Category</option>

                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Company Name</label>
                                        <input type="text" class="form-control form-input" id="employer" />

                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Job Title</label>
                                        <input type="text" class="form-control form-input" id="empDesignation" />
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Start Date</label>
                                        <input type="date" class="form-control form-input" id="empStartDate" />

                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">End Date</label>
                                        <input type="date" class="form-control form-input" id="empEndDate" />
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Home Phone Number</label>
                                        <input type="tel" class="form-control form-input" id="empTelNumber" />
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Email</label>
                                        <input type="email" class="form-control form-input" id="empCompEmail" />
                                    </div>

                                    <div class="form-group">
                                        <label class="control-label">Duties</label>
                                        <input type="text" class="form-control form-input" id="empDuties" />
                                    </div>
                                    <div class="form-group">
                                        &nbsp;
                                    </div>
                                </div>

                                <!-- Modal footer -->
                                <div class="modal-footer">
                                    <button type="button"
                                            class="btn btn-primary"
                                            data-dismiss="modal"
                                            id="addEmpHistory">
                                        Add
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- / Employment Modal-->
                </div>
            </div>
