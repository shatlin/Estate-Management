                                <div class="row">
                                    <div class="col-12 mb-12 text-left">
                                        <button class="btn btn-outline-danger" style="width:250px;" asp-page-handler="Run">Set up</button>
                                    </div>
                                </div>
                            </div>
                    </div>
                </div>
            </div>
        </div>

</form>

